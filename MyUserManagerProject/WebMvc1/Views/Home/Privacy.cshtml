<a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="TestMoreAdminRoleV2">Test More Admin Role V2</a> *@

@section scripts {
    <script src="~/js/Home/privacy.js" asp-append-version="true"></script>
}